{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "meteor": true
    },
    "extends": "@meteorjs/eslint-config-meteor",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "settings": {
        "import/resolver": "meteor"
    }
}